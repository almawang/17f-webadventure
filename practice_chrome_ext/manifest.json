{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "Try injecting stuff",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    //"default_popup": "popup.html",
    "default_title": "Inject some Javascript!"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs",
    "activeTab",
    "storage"
  ]
}